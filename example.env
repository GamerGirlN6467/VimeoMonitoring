# Example .env file for Vimeo Monitor Script

# Vimeo API Access Token
ACCESS_TOKEN=your_vimeo_access_token_here

# Discord Webhook URL
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook_id

# Comma-separated search keywords
SEARCH_QUERIES=keyword1,keyword2,keyword3

# Comma-separated monitored user IDs
MONITORED_USERS=user1,user2,user3

# Known links file name (should match the repository file name)
KNOWN_LINKS_FILE=known_links.txt
